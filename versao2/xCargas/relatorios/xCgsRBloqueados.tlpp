User FUNCTION xCgsRBloqueados()
    private nPag 	:= 1
    PRIVATE _nLin 	:= 0050
    PRIVATE oBrush1 := TBrush():New( , CLR_LIGHTGRAY ) 

    oPrn:= TMSPrinter():New()
    oPrn:setPortrait()
    oPrn:SetPaperSize(9) // A4
    oPrn:Setup()
    oPrn:StartPage()

    CabecBloq(oPrn,2) 
    detailBloq(oPrn,2) 

    _nLin := 0050
    nPag  := 1
    oPrn:EndPage()
    oPrn:StartPage()

    CabecBloq(oPrn,3)
    detailBloq(oPrn,3)

    _nLin := 0050
    nPag  := 1
    oPrn:EndPage()
    oPrn:StartPage()  

    CabecBloq(oPrn,1)
    detailBloq(oPrn,1)

    Set Filter to
    oPrn:EndPage()
    oPrn:Preview()
    MS_FLUSH()


return()   


